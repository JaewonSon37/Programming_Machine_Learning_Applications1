   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "x_new = 23.0 # Set a new spending value for prediction\n",
    "y_new = predict(x_new, slope, intercept) # Predict sales value for the new spending value"
   ]
